<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context="http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	 

	
	<bean id="pointA" class="ques4_5_8_9.spring.assignment.point" init-method="afterPropertiesSet" destroy-method="destroy">
				<property name="x" value="0"/>
				<property name="y" value="0"/>
	</bean>
	
	<bean id="pointB" class="ques4_5_8_9.spring.assignment.point" init-method="afterPropertiesSet" destroy-method="destroy">
				<property name="x" value="10"/>
				<property name="y" value="0"/>
	</bean>
	
	<bean id="pointC" class="ques4_5_8_9.spring.assignment.point" init-method="afterPropertiesSet" destroy-method="destroy">
				<property name="x" value="0"/>
				<property name="y" value="10"/>
	</bean>
	
	<bean id="pointD" class="ques4_5_8_9.spring.assignment.point" init-method="afterPropertiesSet" destroy-method="destroy">
				<property name="x" value="10"/>
				<property name="y" value="10"/>
	</bean>
	
	<bean id="beanLifeCycle" class="ques4_5_8_9.spring.assignment.BeanLifeCycle"></bean>
	
	<context:annotation-config></context:annotation-config>
	<context:component-scan base-package="ques4_5_8_9.spring.assignment"></context:component-scan>
</beans>